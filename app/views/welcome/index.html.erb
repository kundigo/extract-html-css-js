<% content_for (:html_title) {''} %>
<% content_for (:html_desc) {'The Deepcore is a group of physical commodities brokers joining their skills to deliver to the Commodities markets the best trading experience possible.
'} %>

<div class="hero">
    <div>
        <video autoplay loop muted class="hero__video">
            <source src="/images/commodities_transport.mp4" type="video/mp4" alt="Commodities Transport">
        </video>
    </div>
    <div class="hero__content hero__content_homepage">
        <img src="/images/bg-logo.svg" alt="" class="hero__logo-bg">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                  <div class="margin-none">
                    <div class="hero__title hero__title--smaller"><%= html_snippet("homepage.main_section.title", "en") %></div>
                    <div class="hero__text"><%= html_snippet("homepage.main_section.subtitle", "en") %></div>
                  </div>
                </div>
            </div>
        </div>
    </div>
</div>
<section class="page-section page-section_shadow">
    <div class="container">
        <div class="row">
            <div class="col-md-24">
                <div class="section-title section-title_center">
                  <div class="margin-none">
                    <h2 class="section-title__header"><%= html_snippet("homepage.what_we_do_section.title", "en") %></h2>
                    <div class="section-title__sub-header"><%= html_snippet("homepage.what_we_do_section.subtitle", "en") %></div>
                  </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="focus">
                    <div class="focus__img">
                        <img src="/images/home-picto-1.svg" alt="">
                    </div>
                  <div class="margin-none">
                    <div class="focus__title"><%= html_snippet("homepage.what_we_do_section.item1", "en") %></div>
                  </div>
                </div>
            </div>
            <div class="col-md-8">
                <div class="focus">
                    <div class="focus__img">
                        <img src="/images/home-picto-2.svg" alt="">
                    </div>
                  <div class="margin-none">
                    <div class="focus__title"><%= html_snippet("homepage.what_we_do_section.item2", "en") %></div>
                  </div>
                </div>
            </div>
            <div class="col-md-8">
                <div class="focus">
                    <div class="focus__img">
                        <img src="/images/home-picto-3.svg" alt="">
                    </div>
                    <div class="margin-none">
                      <div class="focus__title"><%= html_snippet("homepage.what_we_do_section.item3", "en") %></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="page-section big-data-showcase">
    <div class="container">
        <div class="row">
            <div class="col-md-24">
                <div class="section-title section-title_center">
                    <h2 class="section-title__header section-title__header--simple section-title__header--compact d-inline-block"><span>Big Data</span></h2>
                    <div class="indicator__new--title">New</div>
                    <div class="section-title__header section-title__header--simple">FOB Sugar values worldwide</div>
                </div>
            </div>
        </div>
        <div class="row big-data-showcase__content">
            <div class="col-22 col-lg-10">
                <div class="section__special-content d-flex flex-column align-items-center position-relative">
                    <span class="indicator__new--left">New</span>
                    <div class="fs-5 text-secondary pe-1">Pack</div><br>
                    <div class="showcase__title"><span>Historical</span><br> Values</div>
                    <div class="showcase__body">Get access to all the FOB values since 2017</div>
                    <%= link_to "SEE MORE", data_path(locale: I18n.locale), class: "btn-outline-data" %>
                </div>
            </div>
            <div class="col-22 col-lg-10">
                <div class="section__special-content d-flex flex-column align-items-center position-relative">
                    <span class="indicator__new--left">New</span>
                    <div class="fs-5 text-secondary pe-1">Pack</div><br>
                    <div class="showcase__title"><span>Daily</span><br> Updated Values</div>
                    <div class="showcase__body">Get access to our daily FOB values updated</div>
                    <%= link_to "SEE MORE", data_path(locale: I18n.locale), class: "btn-outline-data" %>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="page-section page-section_bg_gray page-section_padding_30  page-section_shadow">
    <div class="container">
        <div class="text-with-image">
            <div class="row">
                <div class="col-md-12">
                    <div class="text-with-image__image">
                        <img src="/images/femme-data.png" alt="Technology and Physical Commodities Trading">
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="text-with-image__text">
                        <div class="section-title">
                            <div class="section-title__header">
                              <div class="margin-none">
                              <%= html_snippet("homepage.technologie_physical_section.title", "en") %>
                              </div>
                            </div>
                        </div>

                        <div class="text-with-image__content--spaced py-4">
                          <div class="margin-none">
                            <%= html_snippet("homepage.technologie_physical_section.subtitle", "en") %>
                          </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="page-section section-our-products page-section_shadow">
    <div class="container">
        <div class="row">
            <div class="col-md-24">
                <div class="section-title section-title_center">
